type User {
    id: ID!
    userName: String
    fullName: String
}

type Group {
    id: ID!
    name: String
    users: [User]
    admins: [User]
    userIds: [String]
}


type Query {
    user(id: ID!): User
    group(id: ID!): Group
    userGroups(id: ID!): [Group]
    groupDTO(id: ID!): Group
}