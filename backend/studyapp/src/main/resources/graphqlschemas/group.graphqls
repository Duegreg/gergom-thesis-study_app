
type Group {
    id: ID!
    name: String
    description: String
    code: String
    news: News
    users: [User]
    admins: [User]
    liveTests: [LiveTest]
}

type News {
    id: ID!
    text: String
}

extend type Query {
    group(id: ID!): Group
}

extend type Mutation {
    createGroup(name: String!, description: String!): Group
}